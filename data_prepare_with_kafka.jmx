              <stringProp name="BeanShellSampler.query">import java.io.File;

String filename = vars.get(&quot;Topic_Name&quot;) + &quot;_pa&quot; + vars.get(&quot;Partition_index&quot;) + &quot;.csv&quot;;

vars.put(&quot;users_info_csv&quot;, filename);

File myfile = new File(filename);

if(!myfile.exists()) {
	log.info(&quot;### file does not exsit..&quot;);
} else {
	myfile.delete();
	log.info(&quot;### file is deleted..&quot;);
}
</stringProp>
