                <stringProp name="BeanShellSampler.query">import org.apache.commons.codec.binary.Base64;
import java.io.FileWriter;


log.info(&quot;### decode base64 user info&quot;);

vars.put(&quot;user_record&quot;, new String(Base64.decodeBase64(vars.get(&quot;cur_user_encoded&quot;))));

log.info(&quot;### append user info to csv file&quot;);
String csvFile = vars.get(&quot;users_info_csv&quot;); // defined in Check &amp; Delete file step

FileWriter fileWriter = new FileWriter(csvFile, true);

fileWriter.append(vars.get(&quot;user_record&quot;));
fileWriter.append(&quot;\n&quot;);

fileWriter.flush();
fileWriter.close();</stringProp>
